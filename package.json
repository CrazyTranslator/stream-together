{
  "name": "stream-together",
  "version": "0.1.3",
  "scripts": {
    "start": "npm run startServer",
    "devClient": "vite",
    "buildClient": "vite build",
    "startServer": "ts-node ./src/server/index.ts",
    "buildServer": "tsc",
    "devServer": "nodemon startServer",
    "buildPlugin": "webpack",
    "watchPlugin": "webpack --watch",
    "lint": "eslint src"
  },
  "dependencies": {
    "@types/chrome": "0.0.122",
    "@types/express": "^4.17.6",
    "@types/express-ws": "^3.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.22",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "morgan": "^1.10.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "uuid": "^8.2.0",
    "vue": "^3.0.0-rc.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "eslint": "^7.8.1",
    "json-post-process-webpack-plugin": "^2.0.4",
    "nodemon": "^2.0.4",
    "ts-loader": "^8.0.3",
    "vite": "^1.0.0-rc.1",
    "vue-eslint-parser": "^7.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "zip-webpack-plugin": "^3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src/server"
    ],
    "exec": "npm run startServer",
    "ext": "ts"
  }
}
