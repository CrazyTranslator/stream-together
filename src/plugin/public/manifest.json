{
  "name": "stream-together",
  "author": "Michael David Kuckuk",
  "manifest_version": 2,
  "options_page": "options/index.html#/options",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "backgroundScript.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Stream Together",
    "default_popup": "options/index.html#/popup"
  },
  "permissions": [
    "http://*/",
    "ws://*/",
    "https://*/",
    "wss://*/",
    "storage"
  ]
}
